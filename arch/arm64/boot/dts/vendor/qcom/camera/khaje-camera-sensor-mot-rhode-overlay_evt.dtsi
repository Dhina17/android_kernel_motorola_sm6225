/delete-node/ &cam_sensor_mclk0_active;
/delete-node/ &cam_sensor_mclk1_active;
/delete-node/ &cam_sensor_mclk2_active;
/delete-node/ &cam_sensor_mclk3_active;
/delete-node/ &cam_sensor_rear0_reset_active;
/delete-node/ &cam_sensor_front_reset_active;
/delete-node/ &cam_sensor_rear_macro_reset_active;
/delete-node/ &cam_sensor_rear_wide_reset_active;
/delete-node/ &cam_sensor_mclk0_suspend;
/delete-node/ &cam_sensor_mclk1_suspend;
/delete-node/ &cam_sensor_mclk2_suspend;
/delete-node/ &cam_sensor_mclk3_suspend;
/delete-node/ &cam_sensor_rear0_reset_suspend;
/delete-node/ &cam_sensor_front_reset_suspend;
/delete-node/ &cam_sensor_rear_macro_reset_suspend;
/delete-node/ &cam_sensor_rear_wide_reset_suspend;
